    <ItemGroup>
      <PackageReference Include="Zaabee.RabbitMQ.ISerialize" Version="1.1.0" />
      <PackageReference Include="Zaabee.SystemTextJson" Version="2.1.0" />
    </ItemGroup>

</Project>
