    <ItemGroup>
      <PackageReference Include="Zaabee.RabbitMQ.ISerialize" Version="1.0.1.1" />
      <PackageReference Include="Zaabee.Utf8Json" Version="1.0.0" />
    </ItemGroup>

</Project>
