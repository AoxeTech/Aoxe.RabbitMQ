<Project Sdk="Microsoft.NET.Sdk">
    
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2021.1.0</PackageVersion>
        <Version>2021.1.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaaby Zaabee MessageQueue MQ MessageBus RabbitMQ</PackageTags>
        <PackageProjectUrl>https://github.com/Mutuduxf/Zaabee.RabbitMQ</PackageProjectUrl>
        <Description>Rabbit client wrapper for asp.net core or Zaaby</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <LangVersion>Latest</LangVersion>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="RabbitMQ.Client" Version="6.2.1" />
        <PackageReference Include="Zaabee.RabbitMQ.Abstractions" Version="2021.1.0" />
        <PackageReference Include="Zaabee.RabbitMQ.Serializer.Abstraction" Version="2021.1.0" />
    </ItemGroup>
    
</Project>