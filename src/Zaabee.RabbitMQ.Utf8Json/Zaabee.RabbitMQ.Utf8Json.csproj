    <ItemGroup>
      <PackageReference Include="Zaabee.RabbitMQ.ISerialize" Version="1.1.1" />
      <PackageReference Include="Zaabee.Utf8Json" Version="2.2.0" />
    </ItemGroup>

</Project>
