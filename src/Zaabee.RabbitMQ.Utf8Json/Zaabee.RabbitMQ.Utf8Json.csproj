        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.2.2</PackageVersion>
        <Version>1.2.2</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaabee MessageQueue MQ RabbitMQ Json Utf8Json</PackageTags>
        <Description>The serializer for Zaabee.RabbitMQ by Zaabee.RabbitMQ.ISerialize</Description>
