        <PackageVersion>2021.1.3</PackageVersion>
        <Version>2021.1.3</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaaby Zaabee MessageQueue MQ MessageBus RabbitMQ Xml</PackageTags>
